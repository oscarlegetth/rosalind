   "source": [
    "input_file = open(\"rosalind_fib.txt\", \"r\")\n",
    "line = input_file.readline()\n",
    "words = line.split(\" \")\n",
    "\n",
    "f_n1 = 1\n",
    "f_n2 = 0\n",
    "n = int(words[0])\n",
    "k = int(words[1])\n",
    "\n",
    "for i in range(1, n):\n",
    "    f_n = f_n1 + f_n2 * k\n",
    "    f_n2 = f_n1\n",
    "    f_n1 = f_n\n",
    "\n",
    "print(f_n)"
   ]
